  "entity_action": {
    "type": "origins:area_of_effect",
    "radius": 32,
    "bientity_action": {
      "type": "origins:target_action",
      "action": {
        "type": "origins:heal",
        "amount": 2
      }
    },
    "bientity_condition": {
      "type": "origins:target_condition",
      "entity_condition": {
        "type": "origins:or",
        "conditions": [
          {
            "type": "origins:entity_type",
            "entity_type": "minecraft:player"
          },
          {
            "type": "origins:entity_type",
            "entity_type": "minecraft:villager"
          }
        ]
      }
    }
  }
