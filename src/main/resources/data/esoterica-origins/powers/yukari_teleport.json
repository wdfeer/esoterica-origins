{
  "name": "Gap Lady",
  "description": "Activate every 60 seconds to teleport target entity within 64 blocks 50 blocks into the air. Teleport yourself instead if sneaking.",
  "type": "origins:active_self",
  "entity_action": {
    "type": "origins:if_else",
    "condition": {
      "type": "origins:sneaking"
    },
    "if_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:spawn_particles",
          "particle": {
            "type": "minecraft:portal"
          },
          "count": 16,
          "speed": 0.2,
          "force": false,
          "spread": {
            "x": 1.0,
            "y": 1.5,
            "z": 1.0
          }
        },
        {
          "type": "origins:execute_command",
          "command": "tp @s ~ ~50 ~"
        }
      ]
    },
    "else_action": {
      "type": "origins:raycast",
      "distance": 64,
      "block": true,
      "entity": true,
      "fluid_handling": "none",
      "bientity_action": {
        "type": "origins:target_action",
        "action": {
          "type": "origins:execute_command",
          "command": "tp @s ~ ~50 ~"
        }
      },
      "command_at_hit": "particle portal ~ ~ ~ 0.3 0.3 0.3 1 32 normal",
      "command_along_ray": "particle block nether_portal ~ ~ ~ 0.05 0.05 0.05 1 2 normal",
      "command_step": 1
    }
  },
  "cooldown": 60,
  "hud_render": {
    "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_03.png",
    "bar_index": 7
  },
  "key": {
    "key": "key.origins.secondary_active"
  }
}
